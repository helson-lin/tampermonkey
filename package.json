{
  "name": "tampermonkey-template",
  "version": "1.0.0",
  "description": "this is a template for tampermonkey developer",
  "main": "src/main.js",
  "scripts": {
    "dev": "nodemon",
    "build:dev": "npx webpack --config webpack.dev.js",
    "build:prod": "npx webpack --config webpack.prod.js",
    "build:css": "npx tailwindcss -i ./src/assets/global.css -o ./src/main.css",
    "eslint": "eslint --init",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "tampermonkey",
    "vue",
    "tailwindcss"
  ],
  "author": "helson-lin",
  "license": "MIT",
  "devDependencies": {
    "babel-loader": "^9.1.3",
    "css-loader": "^7.1.1",
    "eslint": "^9.2.0",
    "tailwindcss": "^3.4.3",
    "vue": "^3.4.26",
    "vue-loader": "^17.4.2",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4"
  }
}
